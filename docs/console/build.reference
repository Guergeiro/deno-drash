#!/bin/bash

(
  export DRASH_DIR_EXAMPLE_CODE="$DRASH_DIR_ROOT/docs/src/example-code"
  export DRASH_CORE_LOGGER_ENABLED=true
  export DRASH_CORE_LOGGER_LEVEL="debug"

  echo "Running build.reference.ts..."
  deno $DRASH_DIR_ROOT/docs/console/build.reference.ts --allow-env --allow-read --allow-write $@

  echo "Done building."
)
